<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Text?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="isen.quiz.view.DeleteController">
    <Text>Enter the ID of person to DELETE</Text>
    <TextField fx:id="idField" promptText="Enter the ID of person to DELETE"/>
    <Button text="Delete Person" onAction="#deletePerson"/>
    <Button layoutX="300" layoutY="70" onAction="#goBack" text="Back"/>
</VBox>
