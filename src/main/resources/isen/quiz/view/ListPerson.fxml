<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="isen.quiz.view.ListController">
    <TableView fx:id="personTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="lastNameColumn" text="Last Name" />
            <TableColumn fx:id="firstNameColumn" text="First Name" />
            <TableColumn fx:id="nickNameColumn" text="Nickname" />
            <TableColumn fx:id="phoneNumberColumn" text="Phone Number" />
            <TableColumn fx:id="addressColumn" text="Address" />
            <TableColumn fx:id="emailAddressColumn" text="Email Address" />
            <TableColumn fx:id="birthDateColumn" text="Birth Date" />
        </columns>
    </TableView>
</AnchorPane>
